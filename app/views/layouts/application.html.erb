<!DOCTYPE html>
<html>
  <head>
    <title>Austana</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="UTF-8">
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= favicon_link_tag image_url('favicon-32x32.png'), :rel => 'icon', :type =>  'image/png' %>
  </head>

  <body>
    <script type="text/javascript">
      window.logo = "<%= image_url('logo.png') %>"
      window.todoList = "<%= image_url('todo-list.svg') %>"
      window.team = "<%= image_url('people.svg') %>"
      window.project = "<%= image_url('project.svg') %>"
      window.austanaBlackOrange = "<%= image_url('austana-black-orange.png') %>"
      window.austanaWhite = "<%= image_url('austana-white.png') %>"
      window.welcome = "<%= image_url('austana-welcome.svg') %>"
      window.welcomeSplash = "<%= image_url('austana-welcome.png') %>"
      window.onboardWelcome = "<%= image_url('welcome.svg') %>"
      window.checkmark = "<%= image_url('checkmark.svg') %>"
    </script>
    <%= yield %>
  </body>
</html>
